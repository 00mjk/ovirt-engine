<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:org.ovirt.engine.ui.common.widget.dialog"
	xmlns:e="urn:import:org.ovirt.engine.ui.common.widget.editor"
	xmlns:k="urn:import:org.ovirt.engine.ui.common.widget.form.key_value" >

	<ui:with field='constants' type='org.ovirt.engine.ui.webadmin.ApplicationConstants' />

	<ui:style type="org.ovirt.engine.ui.webadmin.section.main.view.popup.profile.VnicProfilePopupView.Style">

	@external .gwt-CheckBox;

		.portMirroringEditor .gwt-CheckBox {
			padding: 0px !important;
			border-width: 0px !important;
		}

		.portMirroringEditor {
			width: 235px;
		}

		.publicUseEditor {
			padding: 0px;
			width: 100%;
		}

		.checkBox {
			padding: 0px !important;
			width: 230px !important;
		}


	</ui:style>

	<d:SimpleDialogPanel width="470px" height="320px">
		<d:content>
			<g:FlowPanel>
				<e:ListModelListBoxEditor ui:field="networkEditor" />
				<e:EntityModelTextBoxEditor ui:field="nameEditor" />
				<e:EntityModelTextBoxEditor ui:field="descriptionEditor" />
				<g:SimplePanel width="100%">
					<e:EntityModelCheckBoxEditor ui:field="portMirroringEditor" />
				</g:SimplePanel>
				<g:FlowPanel>
					<k:KeyValueWidget ui:field="customPropertiesSheetEditor"/>
				</g:FlowPanel>
				<e:EntityModelCheckBoxEditor ui:field="publicUseEditor"/>
			</g:FlowPanel>
		</d:content>
	</d:SimpleDialogPanel>

</ui:UiBinder>

