<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:d="urn:import:org.ovirt.engine.ui.webadmin.widget.report">

	<ui:style>
		.frame1 {
		height:300px;
		padding-top:5px;
		width:900px;
		border-width:0px;
		}

		.frame2 {
		height:300px;
		padding-top:5px;
		width:900px;
		border-width:0px;
		}

	</ui:style>

	<g:SimplePanel ui:field="reportsPanel">
		<g:VerticalPanel>
			<d:ReportPostableFrame ui:field="frame1"
				frameName="ReportsFrame1"
				uri="/Resources/embedded_reports/cluster_uptime_by_datacenter_br7a"
				addStyleNames="{style.frame1}"></d:ReportPostableFrame>
			<d:ReportPostableFrame ui:field="frame2"
				frameName="ReportsFrame2" uri="/Resources/embedded_reports/active_entities_by_os_BR18A"
				addStyleNames="{style.frame2}"></d:ReportPostableFrame>

		</g:VerticalPanel>
	</g:SimplePanel>


</ui:UiBinder>
