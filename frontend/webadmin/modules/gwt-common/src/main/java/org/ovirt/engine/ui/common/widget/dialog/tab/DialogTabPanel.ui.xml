<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
    xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:org.gwtbootstrap3.client.ui">

    <b:TabPanel ui:field="tabPanel" addStyleNames="wizard-pf-row dialog_noMax">
        <g:FlowPanel ui:field="tabContainer" addStyleNames="wizard-pf-sidebar dialog_noOverflow">
            <b:NavTabs ui:field="navTabs" addStyleNames="list-group" />
        </g:FlowPanel>
        <g:SimplePanel ui:field="tabHeaderContainer" />
        <b:TabContent ui:field="tabContent" addStyleNames="wizard-pf-main" />
        <b:html.ClearFix />
    </b:TabPanel>

</ui:UiBinder>
